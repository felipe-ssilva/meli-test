{
  "version": 2,
  "name": "Mercado Livre Backend Test",
  "builds": [{ "src": "**/*.js", "use": "@now/node" }],
  "routes": [
    {
      "headers": {
        "Access-Control-Allow-Origin": "",
        "Access-Control-Allow-Methods": "GET",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      },
      "src": "/.*",
      "dest": "/index.js"
    }
  ]
}
